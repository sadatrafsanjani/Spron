{
	"manifest_version": 3,
	"name": "Spron",
	"version": "1.0",
	"description": "Spring cron expression generator",
	"icons": {
		"128": "icon/128.png",
		"64": "icon/64.png",
		"48": "icon/48.png"
	},
	"action": {
		"default_icon": "icon/48.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	]
}